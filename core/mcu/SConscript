Import('env')

sources = list()
if env['MCU'].startswith('NRF51'):
    sources.append(File('system_nrf51.c'))

elif env['MCU'].startswith('NRF52'):
    sources.append(File('system_nrf52.c'))

objects = list()
if 'Object' in env['BUILDERS']:
    for s in sources:
        objects.append(env.Object(s))

result = {
    'objects' : objects
}
Return('result')