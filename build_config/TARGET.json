{
  "target": [
    {
      "cortex_m0": [
        {
          "CCFLAGS": [
            "-mcpu=cortex-m0"
          ]
        },
        {
          "LDFLAGS": [
            "-mcpu=cortex-m0"
          ]
        }
      ]
    },
    {
      "cortex_m4": [
        {
          "CCFLAGS": [
            "-mcpu=cortex-m4"
          ]
        },
        {
          "LDFLAGS": [
            "-mcpu=cortex-m4"
          ]
        }
      ]
    },
    {
      "win32": [
        {
          "CCFLAGS": [
            "-arch i386",
            "-DWIN32",
            "-D_WIN32",
            "-Wno-implicit-function-declaration",
            "-Wno-duplicate-decl-specifier",
            "-Wno-uninitialized"
          ]
        },
        {
          "LDFLAGS": [
            "-arch i386",
            "-Wno-implicit-function-declaration",
            "-Wno-duplicate-decl-specifier",
            "-Wno-uninitialized"
          ]
        }
      ]
    },
    {
      "linux": [
        {
          "CCFLAGS": [
            "-m32"
          ]
        },
        {
          "LDFLAGS": [
            "-m32"
          ]
        }
      ]
    }
  ]
}