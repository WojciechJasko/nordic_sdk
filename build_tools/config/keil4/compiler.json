{
  "compiler": {
    "CCFLAGS": [
      "-mthumb",
      "-mabi=aapcs",
      "-Wall",
      "-Werror",
      "-g3",
      "-ffunction-sections",
      "-fdata-sections",
      "-fno-strict-aliasing",
      "-fno-builtin",
      "--short-enums",
      "-MP",
      "-MD"
    ],
    "LINKFLAGS": [
      "-mthumb",
      "-mabi=aapcs",
      "-Wl",
      "--gc-sections",
      "--specs=nano.specs",
      "-lc",
      "-lnosys"
    ],
    "CC": "ARM\BIN40\Armcc.Exe",
    "CXX": "ARM\BIN40\Armcc.Exe",
    "AS": "ARM\BIN40\Armasm.Exe",
    "AR": "ARM\BIN40\ArmAr.Exe",
    "LD": "ARM\BIN40\ArmLink.Exe",
    "NM": "ARM\BIN40\ArmAr.Exe",
    "OBJDUMP": "ARM\BIN40\FromElf.Exe",
    "OBJCOPY": "arm-none-eabi-objcopy",
    "SIZE": "arm-none-eabi-size",
    "RANLIB": "arm-none-eabi-ranlib",
    "PROGSUFFIX": ".elf",
    "CCCOM": "r'$CC -I $KEILR\ARM\INC -I $KEILR\ARM\INC\ARM $CFLAGS $CCFLAGS $CPPFLAGS $_CPPDEFFLAGS $_CPPINCFLAGS -c -o $TARGET $SOURCES'",
    "ASPPFLAGS": " '$ASFLAGS'",
    "ASCOM"    : " '$AS $ASFLAGS -o $TARGET $SOURCES'",
    "ASPPCOM"  : " '$CC $ASPPFLAGS $CPPFLAGS $_CPPDEFFLAGS $_CPPINCFLAGS -c -o $TARGET $SOURCES'"
  }
}


!!!use these values: http://scons.tigris.org/issues/show_bug.cgi?id=2930
